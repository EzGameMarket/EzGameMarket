@model DetailProduct

<div class="single-product">
    <!-- Container -->
    <div class="container">
        <!-- Row -->
        <div class="row">

            <div class="col-lg-9 col-md-8 product-info">
                @await Html.PartialAsync("ProductsPartial/_ProductInDetail", Model)
            </div>
            <div class="col-lg-3 col-md-4">
                @await Html.PartialAsync("SideBarsPartial/_RecomendedProducts", Model)
            </div>
        </div>
    </div>
</div>