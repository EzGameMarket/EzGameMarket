@page
@model WebMVC.Views.Account.RegisterModel
@{
}

<div id="register" class="tab-pane fade">
    <div class="account-form-container register-form">
        <div class="account-form">
            <form action="#" method="post">
                <input type="text" required="" name="first-name" class="form-control" placeholder="First Name">
                <input type="text" required="" name="last-name" class="form-control" placeholder="Last Name">
                <input name="user-email" required="" type="email" class="form-control" placeholder="Email">
                <input type="password" required="" class="form-control" placeholder="Password">
                <div class="button-box">
                    <button type="submit" class="btn btn-default btn-normal"><span>Register</span></button>
                </div>
            </form>
        </div>
    </div>
</div>